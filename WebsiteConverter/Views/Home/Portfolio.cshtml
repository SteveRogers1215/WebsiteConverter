            <div class="row portfolio-container" data-aos="fade-up" data-aos-delay="200">
                @foreach (ProjectViewModel item in ViewBag.Project)
                {
                    <div class="col-lg-4 col-md-6 portfolio-item filter-app">
                        <div class="portfolio-wrap">
                            <img src="~/assets/img/portfolio/@item.Thumbnail" class="img-fluid" alt="">
                            <div class="portfolio-info">
                                <h4>@item.Name</h4>                                
                                <div class="portfolio-links">
                                    <a href="@Url.Action("PortfolioDetails", "Home", new {id=item.ID})" title="Portfolio Details"><i class="bx bx-link"></i></a>
                                </div>
                            </div>
                        </div>
                    </div>
                }
                

